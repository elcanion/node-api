{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "test:staged": "jest --passWithNoTests",
    "test:ci": "jest --coverage"
  },
  "standard": {
    "env": ["jest"]
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "npm run test:staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elcanion/node-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elcanion/node-api/issues"
  },
  "homepage": "https://github.com/elcanion/node-api#readme",
  "devDependencies": {
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.2",
    "standard": "^17.0.0"
  }
}
